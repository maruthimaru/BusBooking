<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark">


    <TextView
        android:text="Available Seats"
        android:layout_width="wrap_content"
        android:id="@+id/textView"
        android:layout_height="50dp"
        android:textSize="18sp"
        android:textColor="?android:attr/colorPressedHighlight"
        android:gravity="bottom|left|center_vertical"
        android:layout_above="@+id/button3"
        android:layout_toEndOf="@+id/button2"
        tools:textColor="#FFFFFF" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="28dp"
        android:layout_height="25dp"
        android:id="@+id/button2"
        android:layout_alignParentStart="true"
        android:backgroundTint="#FFFFFF"
        />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="28dp"
        android:layout_height="25dp"
        android:id="@+id/button3"
        android:layout_below="@+id/button2"
        android:layout_alignParentStart="true"
        android:backgroundTint="#A45B5B" />

    <TextView
        android:text="Sold  Seats"
        android:layout_width="wrap_content"
        android:id="@+id/textView2"
        android:layout_height="50dp"
        android:textSize="18sp"
        android:textColor="#A45B5B"
        android:gravity="bottom|left|center_vertical"
        android:layout_below="@+id/textView"
        android:layout_toEndOf="@+id/button3"
        android:layout_alignBottom="@+id/button3" />
    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:id="@+id/O1"
        android:background="#FFFFFF"
        android:text="18"
        android:textSize="10dp"
        android:layout_alignTop="@+id/O2"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/A2" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/O2"
        android:text="19"
        android:textSize="10dp"
        android:background="#FFFFFF"
        android:layout_above="@+id/A1"
        android:layout_marginBottom="26dp"
        android:layout_alignEnd="@+id/B2" />
    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:id="@+id/A1"
        android:background="#FFFFFF"
        android:text="1"
        android:textSize="10dp"
        android:layout_alignTop="@+id/A2"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/A2" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/A2"
        android:text="2"
        android:textSize="10dp"
        android:background="#FFFFFF"
        android:layout_above="@+id/B1"
        android:layout_marginBottom="26dp"
        android:layout_alignEnd="@+id/B2" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/A3"
        android:text="3"
        android:textSize="10dp"
        android:background="#FFFFFF"
        android:layout_alignTop="@+id/D2"
        android:layout_toEndOf="@+id/textView11" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/A4"
        android:text="4"
        android:textSize="10dp"
        android:background="#FFFFFF"
        android:layout_alignTop="@+id/D3"
        android:layout_toEndOf="@+id/A3" />
    <ImageView
        style="?android:attr/buttonStyleSmall"
        android:layout_width="65dp"
        android:layout_height="65dp"
        android:text="6"
        android:textSize="10sp"
        android:background="#00FFFFFF"
        android:layout_above="@+id/O6"
        android:src="@drawable/ic_steeriongweel"
        android:layout_marginBottom="20dp"
        android:layout_alignEnd="@+id/proceed" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/O5"
        android:text="17"
        android:textSize="10dp"
        android:background="#FFFFFF"
        android:layout_alignTop="@+id/O2"
        android:layout_toStartOf="@+id/A6" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/O6"
        android:text="6"
        android:textSize="10sp"
        android:background="#FFFFFF"
        android:layout_alignTop="@+id/O5"
        android:layout_alignEnd="@+id/proceed" />
    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/A5"
        android:text="17"
        android:textSize="10dp"
        android:background="#FFFFFF"
        android:layout_alignTop="@+id/A2"
        android:layout_toStartOf="@+id/A6" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/A6"
        android:text="6"
        android:textSize="10sp"
        android:background="#FFFFFF"
        android:layout_alignTop="@+id/A5"
        android:layout_alignEnd="@+id/proceed" />


    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="28dp"
        android:layout_height="25dp"
        android:id="@+id/Your"
        android:backgroundTint="#87A96B"
        android:layout_below="@+id/button3"
        android:layout_toStartOf="@+id/textView2"
        />

    <TextView
        android:text="Your  Seats"
        android:layout_width="wrap_content"
        android:id="@+id/textView11"
        android:layout_height="70dp"
        android:textSize="18sp"
        android:textColor="#87A96B"
        android:gravity="bottom|left|center_vertical"
        android:layout_alignTop="@+id/Your"
        android:layout_toEndOf="@+id/Your"
        android:layout_alignBottom="@+id/Your"
        android:backgroundTint="#87A96B"
        />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="100dp"
        android:layout_height="40dp"
        android:id="@+id/proceed"
        android:backgroundTint="#FFFFFF"
        android:text="Next"
        android:layout_marginBottom="24dp"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/textView13"
        android:background="#FFFFFF"/>

    <TextView
        android:id="@+id/textView13"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_above="@+id/proceed"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="24dp"
        android:textColor="?android:attr/colorPressedHighlight"
        android:textSize="20dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/B1"
        android:text="5"
        android:textSize="10dp"
        android:background="#FFFFFF"
        android:layout_above="@+id/C1"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="26dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/B2"
        android:text="6"
        android:textSize="10dp"
        android:layout_alignTop="@+id/B1"
        android:layout_toEndOf="@+id/B1"
        android:background="#FFFFFF"/>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/B3"
        android:text="7"
        android:textSize="10dp"
        android:background="#FFFFFF"
        android:layout_alignTop="@+id/B2"
        android:layout_alignStart="@+id/A5" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/B4"
        android:text="8"
        android:textSize="10dp"
        android:layout_alignTop="@+id/B3"
        android:layout_toEndOf="@+id/B3"
        android:background="#FFFFFF"/>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/C1"
        android:text="9"
        android:textSize="10dp"
        android:background="#FFFFFF"
        android:layout_alignTop="@+id/C3"
        android:layout_alignParentStart="true" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/C2"
        android:text="10"
        android:textSize="10dp"
        android:layout_alignTop="@+id/C1"
        android:layout_toEndOf="@+id/C1"
        android:background="#FFFFFF"/>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/C3"
        android:text="11"
        android:textSize="10dp"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/B4"
        android:background="#FFFFFF"/>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/C4"
        android:text="12"
        android:textSize="10dp"
        android:layout_alignTop="@+id/C3"
        android:layout_alignStart="@+id/B4"
        android:background="#FFFFFF"/>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/D1"
        android:text="13"
        android:textSize="10dp"
        android:layout_below="@+id/C1"
        android:layout_alignStart="@+id/C1"
        android:layout_marginTop="29dp"
        android:background="#FFFFFF" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/D2"
        android:text="14"
        android:textSize="10dp"
        android:layout_alignTop="@+id/D1"
        android:layout_toEndOf="@+id/D1"
        android:background="#FFFFFF" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/D3"
        android:text="15"
        android:textSize="10dp"
        android:layout_alignTop="@+id/D2"
        android:layout_alignStart="@+id/C3"
        android:background="#FFFFFF"/>

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="45dp"
        android:layout_height="25dp"
        android:id="@+id/D4"
        android:text="16"
        android:textSize="10dp"
        android:layout_alignTop="@+id/D3"
        android:layout_toEndOf="@+id/D3"
        android:background="#FFFFFF"/>



</RelativeLayout>
